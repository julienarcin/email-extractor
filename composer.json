{
    "name": "julienarcin/email-extractor",
    "description": "Email extractor from Julien Arcin.",
    "homepage": "https://scrap.io",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Arcin",
            "email": "julien@scrap.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "laminas/laminas-servicemanager": "^2.6",
        "laminas/laminas-text": "^2.5",
        "laravel-zero/framework": "^10.0.2",
        "nunomaduro/termwind": "^1.15.1"
    },
    "require-dev": {
        "laravel/pint": "^1.8",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^2.5",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": [
            "php-cs-fixer fix --show-progress=estimating --config=.php_cs.dist"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["email-extractor"]
}
